{
  "name": "@concordant/c-service",
  "version": "1.1.3",
  "description": "Background service for the Concordant platform",
  "keywords": [
    "concordant",
    "crdt",
    "conflict-free",
    "replicated datatypes"
  ],
  "homepage": "https://concordant.io",
  "author": "Concordant",
  "bugs": {
    "email": "support@concordant.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.inria.fr/concordant/software/c-service.git"
  },
  "bin": "./dist/service.js",
  "main": "./dist/service.js",
  "types": "./dist/service.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "dist/service.js",
    "test": "jest $TESTS",
    "lint": "eslint .",
    "build": "tsc --skipLibCheck && chmod +x dist/service.js",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.8",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.164",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@concordant/c-crdtlib": "^1.0.4",
    "@types/uuid": "^3.4.9",
    "apollo-server-express": "^2.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "graphql-tools": "^4.0.8",
    "lodash": "^4.17.20",
    "nano": "^8.2.3",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-asyncstorage": "^6.4.1",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-react-native": "^7.0.0-beta-1",
    "sofa-api": "^0.5.1",
    "uuid": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md,json,yml}": "prettier --write"
  }
}
